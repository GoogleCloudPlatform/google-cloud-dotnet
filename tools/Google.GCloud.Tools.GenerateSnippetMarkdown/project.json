{
  "description": "Console app to generate Markdown files for docfx with snippet references",

  "buildOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "YamlDotNet": "3.8.0",
    "docfx": "2.3.0"
  },

  "commands": {
    "run": "Google.GCloud.Tools.GenerateSnippetMarkdown"
  },

  "frameworks": {
    "net451": {
      "imports": "dnx451"
    }
  }
}
