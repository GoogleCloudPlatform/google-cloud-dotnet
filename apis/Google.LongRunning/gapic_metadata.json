              "methods": [
                "ListOperations",
                "ListOperationsAsync"
              ]
            },
            "GetOperation": {
              "methods": [
                "GetOperation",
                "GetOperationAsync"
              ]
            },
            "DeleteOperation": {
              "methods": [
                "DeleteOperation",
                "DeleteOperationAsync"
              ]
            },
            "CancelOperation": {
              "methods": [
                "CancelOperation",
                "CancelOperationAsync"
              ]
            },
            "WaitOperation": {
              "methods": [
                "WaitOperation",
                "WaitOperationAsync"
              ]
            }
          }
        }
      }
    }
  }
}
